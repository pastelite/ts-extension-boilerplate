{

  "manifest_version": 3,
  "name": "extension boilerplate",
  "version": "1.0",

  "description": "This extension is for testing purpose",

  "icons": {
    "48": "icons/icon.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon.png",
      "32": "icons/icon.png"
    },
    "default_title": "Borderify",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "scripting",
    "activeTab"
  ]
}
